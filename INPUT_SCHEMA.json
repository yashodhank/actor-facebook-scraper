        "commentsMode": {
            "title": "Comments mode",
            "description": "Choose the way the comments are sorted",
            "type": "string",
            "default": "RANKED_THREADED",
            "editor": "select",
            "enum": ["RANKED_THREADED", "RECENT_ACTIVITY", "RANKED_UNFILTERED"],
            "enumTitles": ["Most relevant", "Newest first", "Non-filtered"]
        },
        "maxPosts": {
            "title": "Max posts",
            "description": "Limit the max number of posts to return",
            "type": "integer",
            "default": 3,
            "prefill": 3,
            "editor": "number"
        },
        "maxPostDate": {
            "title": "Max post date",
            "description": "Limit the date of the posts",
            "example": "2020-01-01",
            "prefill": "2020-01-01",
            "type": "string",
            "editor": "textfield",
            "pattern": "\\d{4}-\\d{2}-\\d{2}"
        },
        "maxPostComments": {
            "title": "Max comments",
            "description": "Limit the max comments per post to return",
            "type": "integer",
            "default": 15,
            "prefill": 15,
            "editor": "number"
        },
        "maxCommentDate": {
            "title": "Max comment date",
            "description": "Limit the minimum date for comments",
            "example": "2020-01-01",
            "prefill": "2020-01-01",
            "type": "string",
            "editor": "textfield",
            "pattern": "\\d{4}-\\d{2}-\\d{2}"
        },
        "maxReviews": {
            "title": "Max reviews",
            "description": "Limit the max number of reviews to return",
            "type": "integer",
            "default": 3,
            "prefill": 3,
            "editor": "number"
        },
        "maxReviewDate": {
            "title": "Max review date",
            "description": "Limit the date of the reviews",
            "prefill": "2020-01-01",
            "example": "2020-01-01",
            "type": "string",
            "editor": "textfield",
            "pattern": "\\d{4}-\\d{2}-\\d{2}"
        },
